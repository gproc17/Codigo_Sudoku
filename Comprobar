using namespace std;

void imprimir_matriz_9x9solotablero(int matriz[9][9]){
    char x=0;
    cout << "Ingresar un borde: " << endl;
    cin >> x;
    for (int i=0; i<19; i++){
        cout << x;
    }
    cout << endl;
    for (int i=0; i<9;++i){
        for(int j=0;j<9; ++j){

            if (j==2||j==5||j==8)
                cout  << ' ' << x;
            else if (j==0)
                cout  << x <<' ' <<"|";
            else
                cout  << ' '<< "|";
        }
        cout<<endl;
        if (i==2||i==5||i==8) {
            for (int i=0; i<19; i++) {
                cout << x;
            }
            cout << endl;
        } else{
            cout << x << "-+-+-" << x << "-+-+-" << x<< "-+-+-" << x<<endl;
        }
    }
}

void imprimir_matriz_9x9(int matriz[9][9]){
    char x=0;
    cout << "Ingresar un borde: " << endl;
    cin >> x;
    for (int i=0; i<19; i++){
        cout << x;
    }
    cout << endl;
    for (int i=0; i<9;++i){
        for(int j=0;j<9; ++j){

            if (j==2||j==5||j==8)
                cout  << matriz[i][j] << x;
            else if (j==0)
                cout  << x <<matriz[i][j] <<"|";
            else
                cout  << matriz[i][j] << "|";
        }
        cout<<endl;
        if (i==2||i==5||i==8) {
            for (int i=0; i<19; i++) {
                cout << x;
            }
            cout << endl;
        } else{
            cout << x << "-+-+-" << x << "-+-+-" << x<< "-+-+-" << x<<endl;
        }
    }
}

bool buscarfilas(int M[9][9]){
    for (int i=0;i<9;i++){
        for (int j=0;j<9;j++){
            for (int k=0;k<9;k++){
                if (j!=k && M[i][j]==M[i][k]) {

                    return false;
                }
                if (i!=k && M[i][j]==M[k][j]) {

                    return false;
                }
            }

        }
    }
    return true;
}

int cambiarcuadranteamatriz(int M[9][9],int M1[9][9]){
    for (int i=0;i<9;i++){
        for (int j=0;j<9;j++){
            if (i<3&&j<3){  //1
                M1[i][j]=M[i][j];
            }else if (i<3&&j>2&&j<6){    //2
                M1[i][j]=M[i][j];
            }else if (i<3&&j>5&&j<9){   //3*
                M1[i][j]=M[i][j];
            }else if (i>2&&i<6&&j<3){   //4
                M1[i][j]=M[i][j];
            }else if (i>2&&i<6&&j>2&&j<6){   //5
                M1[i][j]=M[i][j];
            }else if (i>2&&i<6&&j>5&&j<9){    //6
                M1[i][j]=M[i][j];
            }else if (i>5&&i<9&&j<3){   //7
                M1[i][j]=M[i][j];
            }else if (i>5&&i<9&&j>2&&j<6){   //8
                M1[i][j]=M[i][j];
            }else if (i>5&&i<9&&j>5&&j<9){    //9
                M1[i][j]=M[i][j];
            }

        }
    }
    return 0;
}

bool buscarcuadrante(int M[9][9],int M1[9][9]){
    cambiarcuadranteamatriz(M,M1);
    buscarfilas(M1);
    return true;
}



int main() {
    char a;
    cout<<"Si quiere imprimir el teblero escriba '0', de lo contrario escriba cuelquier nÃºmero";
    cin>>a;
    int M[9][9];
    int M1[9][9];
    if(a=='0'){
        imprimir_matriz_9x9solotablero(M);
    }else{int M[9][9]={2, 1, 7, 3, 5, 8, 6, 9, 4,
                       3, 4, 5, 7, 6, 9, 8, 1, 2,
                       6, 8, 9, 1, 4, 2, 3, 7, 5,
                       8, 6, 1, 5, 3, 7, 4, 2, 9,
                       5, 9, 3, 8, 2, 4, 7, 6, 1,
                       4, 7, 2, 6, 9, 1, 5, 8, 3,
                       9, 5, 6, 2, 8, 3, 1, 4, 7,
                       7, 2, 8, 4, 1, 5, 9, 3, 6,
                       1, 3, 4, 9, 7, 6, 2, 5, 8};
        imprimir_matriz_9x9(M);
        if (buscarfilas(M)== false || buscarcuadrante(M,M1) == false)
            cout<<"no es sudoku";
        else
            cout<<"es sudoku";
    }
    return 0;
}
